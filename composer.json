{
    "name": "fabiopellati/apigility-tools-cli",
    "description": "Cli command tool for development with apigility-tools",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Fabio Pellati",
            "email": "fabio.pellati@gmail.com"
        }
    ],
    "repositories": [],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "symfony/console": "^5.2",
        "laminas/laminas-code": "^3.5",
        "laminas/laminas-stdlib": "^3.3",
        "laminas/laminas-filter": "^2.10",
        "laminas/laminas-config": "^3.4",
        "laminas/laminas-config-aggregator": "^1.4"
    },
    "require-dev": {
        "laminas-api-tools/api-tools-admin": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "ApigilityTools\\Cli\\": "src/"
        }
    },
    "bin": [
        "bin/apigility-tools-cli"
    ],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "extra": {
        "laminas": {
            "component": []
        }
    }
}
